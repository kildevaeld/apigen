
import ./models.api

get /blog {
    returns [Blog]
}

post /blog {
    returns Blog
    body {
        title string
        body string
    }
    auth token
}

get /blog/:id { returns Blog? }

put /blog/:id { 
    returns Blog
    body Blog
    headers [
        content-type
    ]
}

delete /blog/:id { 
    returns void
}